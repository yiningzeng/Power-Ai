{
    "type": "object",
    "properties": {
        "netModelType": {
            "type": "string",
            "title": "网络模型",
            "description": "用于训练的网络模型",
            "enum": [
                "maskRcnn",
                "retinaNet",
                "fasterRcnn"
            ],
            "default": "fasterRcnn",
            "enumNames": [
                "Mask R-CNN",
                "RetinaNet",
                "Faster RCNN"
            ]
        },
        "layerNumbEnum": {
            "type": "number",
            "title": "网络层数",
            "enum": [
                50,
                101
            ],
            "default": 50
        },
        "gpuNumb": {
            "type": "integer",
            "title": "GPU数量",
            "default": 1,
            "minimum": 1
        },
        "dataEnhancement": {
            "title": "数据增强",
            "type": "boolean",
            "description": "训练的时候模拟大量数据训练",
            "default": true
        },
        "multiScale": {
            "title": "多尺度",
            "type": "boolean",
            "description": "多尺度训练",
            "default": true
        },
        "useFlipped": {
            "title": "图像翻转",
            "type": "boolean",
            "description": "开启多尺度尽量不要打开图像翻转",
            "default": false
        }
    }
}
