{
    "type": "object",
    "title": "${strings.train.providers.fasterRcnn.displayName}",
    "properties": {
        "fasterRcnnImage": {
            "type": "string",
            "title": "镜像版本",
            "description": "训练镜像使用的版本",
            "default": "registry.cn-hangzhou.aliyuncs.com/baymin/ai-power:ai-power-wo-auto-v3.6"
        },
        "detectron": {
            "title": "网络参数",
            "type": "object",
            "properties": {
                "layerNumbEnum": {
                    "type": "number",
                    "title": "网络层数",
                    "enum": [
                        50,
                        101
                    ],
                    "default": 50
                },
                "gpuNumb": {
                    "type": "number",
                    "title": "GPU数量",
                    "enum": [
                        1,
                        2,
                        4,
                        8
                    ],
                    "default": 1
                },
                "fpn": {
                    "title": "FPN",
                    "type": "boolean",
                    "description": "开启FPN",
                    "default": true
                },
                "augument": {
                    "title": "数据增强",
                    "type": "boolean",
                    "description": "训练的时候模拟大量数据训练",
                    "default": true
                },
                "multiScale": {
                    "title": "多尺度",
                    "type": "boolean",
                    "description": "多尺度训练",
                    "default": true
                },
                "useFlipped": {
                    "title": "图像翻转",
                    "type": "boolean",
                    "description": "开启多尺度尽量不要打开图像翻转",
                    "default": false
                }
            }
        }
    }
}
